package cursojava.aula27;

public class ContaCorrente {
	String ContaCorrente;
	double saldo;
	boolean special;
	double limit;
		
	saldo = saldo + limit;

void VerificarSaldoSaque() {
	
	if(saldo >= 0) {
		 System.out.println("Você possui " + saldo + " para saque");
	}

}



}

	

